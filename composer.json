{
    "name": "fabriscorol/bj_phone_number",
    "type": "library",
    "description": "A minimal module to validate Beninese mobile phone numbers.",
    "keywords": [
        "fabriscorol",
        "bj_phone_number"
    ],
    "homepage": "https://github.com/fabriscorol/bj_phone_number",
    "license": "MIT",
    "authors": [
        {
            "name": "Fabrice TOHIONON",
            "email": "fabrice@tohionon.net",
            "homepage": "https://tohionon.net",
            "role": "Developer"
        }
    ],
    "require": {
        "php" : "~7.1"
    },
    "require-dev": {
        "phpunit/phpunit" : ">=7.0",
        "squizlabs/php_codesniffer": "^3.0"
    },
    "autoload": {
        "psr-4": {
            "fabriscorol\\bj_phone_number\\": "src"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "fabriscorol\\bj_phone_number\\": "tests"
        }
    },
    "scripts": {
        "test": "phpunit",
        "check-style": "phpcs src tests",
        "fix-style": "phpcbf src tests"
    },
    "extra": {
        "branch-alias": {
            "dev-master": "1.0-dev"
        }
    },
    "config": {
        "sort-packages": true
    }
}
